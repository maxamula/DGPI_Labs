<Window x:Class="DGPI_Labs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DGPI_Labs"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Width="600" HorizontalAlignment="Center" VerticalAlignment="Center">
        <WrapPanel>
            <Label Content="Label:" Width="100" HorizontalAlignment="Center"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <Label Content="Label:" Width="100" HorizontalAlignment="Center"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
            <TextBox Width="100" Text="TextBox"/>
        </WrapPanel>
        <Button Content="Button" HorizontalAlignment="Center" Width="300"/>
    </StackPanel>
    
</Window>
